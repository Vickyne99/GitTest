                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="COSE">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">FullyExecutedConfirmationSent</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Order is fully executed and the confirmation was sent.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CPRC">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">CancellationRequested</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Cancellation request from the counterparty is awaiting for your cancellation request or your concent.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="DFLA">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">DefaultAction</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Default action is taken.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="DONE">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">Done</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Order is partially filled and no further executions forthcoming for the trading day.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
