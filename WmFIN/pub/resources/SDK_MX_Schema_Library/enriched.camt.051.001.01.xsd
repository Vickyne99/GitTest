            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="CdtAcct" type="AccountIdentificationDetails">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">CreditorAccount</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Account to be credited as a result of a transfer of liquidity. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TrfdAmt" type="AmountChoice">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">TransferredAmount</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Quantity of cash that the transaction administrator transfers from one account to another.</xs:documentation>
