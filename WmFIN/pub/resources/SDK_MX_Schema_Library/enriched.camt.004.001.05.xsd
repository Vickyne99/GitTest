                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="SAPP">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">SAPQueueAmount</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Balance represents the settlement account processor queue amount </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="IRLT">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">IntradayRepoLimit</xs:documentation>
