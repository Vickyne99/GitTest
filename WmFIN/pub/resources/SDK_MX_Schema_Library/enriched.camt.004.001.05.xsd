            <xs:enumeration value="IRDR">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">IntradayRepoDrawings</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Balance represents the intraday repo drawings.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="DWRD">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">DiscountWindowRepoDrawings</xs:documentation>
