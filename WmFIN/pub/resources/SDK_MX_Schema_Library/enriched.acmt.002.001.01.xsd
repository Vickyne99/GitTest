    <xs:simpleType name="CardType1Code">
        <xs:annotation>
            <xs:documentation source="Name" xml:lang="EN">CardType1Code</xs:documentation>
            <xs:documentation source="Definition" xml:lang="EN">Specifies the type of payment card.</xs:documentation>
        </xs:annotation>
