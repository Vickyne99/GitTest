    <xs:complexType name="FinancialInstrument6">
        <xs:annotation>
            <xs:documentation source="Name" xml:lang="EN">FinancialInstrument6</xs:documentation>
            <xs:documentation source="Definition" xml:lang="EN">Security that is a sub-set of an investment fund, and is governed by the same investment fund policy, eg, dividend option or valuation currency.</xs:documentation>
        </xs:annotation>
