    <xs:complexType name="CashAccount4">
        <xs:annotation>
            <xs:documentation source="Name" xml:lang="EN">CashAccount4</xs:documentation>
            <xs:documentation source="Definition" xml:lang="EN">Account to or from which a cash entry is made.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
