                    <xs:documentation source="Name" xml:lang="EN">PaymentType</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Type, or nature, of the payment, eg, express payment.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="PmtInstrRef" type="Max35Text">
