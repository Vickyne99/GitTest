                    <xs:documentation source="Name" xml:lang="EN">TransactionReference</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Unique and unambiguous identifier for a payment transaction, as assigned by the originator. The payment transaction reference is used for reconciliation or to link tasks relating to the payment transaction.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="TrfValDt" type="DateAndDateTimeChoice">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">TransferValueDate</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Date and time at which the cash is at the disposal of the credit account owner, or ceases to be at the disposal of the debit account owner.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="InstrSts" type="PaymentStatusDetails4">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">InstructionStatus</xs:documentation>
