                    <xs:documentation source="Name" xml:lang="EN">PaymentInstructionReference</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Unique and unambiguous identifier for a payment instruction, as assigned by the clearing agent or the initiating party.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="IntrBkValDt" type="ISODate">
