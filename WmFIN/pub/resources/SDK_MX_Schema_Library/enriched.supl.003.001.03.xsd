                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="WSNS">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">NotSupported</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Announcement record is not supported by validation service.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="WSPS">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">PendingNotSupported</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Validation has determined that the record is not supported. Pending authorisations.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="WSPC">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">PendingCancelled</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Validation has determined event is being cancelled by the issuer/offeror. Pending authorisation.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Exact3NumericText">
