        <xs:restriction base="xs:string">
            <xs:enumeration value="SNGL">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">Single</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Indicates that for each occurrences of the payment information block, exactly one occurrence of the payment transaction block is present.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="GRPD">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">Grouped</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Indicates that there is only one occurrence of the payment information block and several occurrences of the payment transaction block.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="MIXD">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">Mixed</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Indicates that there are one or several occurrences of the payment information block where each of the occurences might contain one or several occurrences of the payment transaction block.</xs:documentation>
