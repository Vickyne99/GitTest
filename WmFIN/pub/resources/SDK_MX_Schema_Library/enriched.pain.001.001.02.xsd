            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="PoolgAdjstmntDt" type="ISODate">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">PoolingAdjustmentDate</xs:documentation>
