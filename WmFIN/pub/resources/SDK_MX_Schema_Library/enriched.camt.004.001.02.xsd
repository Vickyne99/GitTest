    <xs:simpleType name="BalanceType4Code">
        <xs:annotation>
            <xs:documentation source="Name" xml:lang="EN">BalanceType4Code</xs:documentation>
            <xs:documentation source="Definition" xml:lang="EN">Specifies the nature of the balance, eg, opening balance.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
