                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">TransactionInformation</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Information concerning the original transactions, to which the cancellation request message refers.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
</xs:schema>
