                    <xs:documentation source="Definition" xml:lang="EN">Provides details on the execution and type of payment contained in the instruction.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TrfdAmt" type="Amount2Choice">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">TransferredAmount</xs:documentation>
