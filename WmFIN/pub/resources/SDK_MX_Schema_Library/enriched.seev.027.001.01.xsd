            </xs:element>
            <xs:choice>
                <xs:element name="AgtCAStgInstrReqId" type="DocumentIdentification8">
                    <xs:annotation>
                        <xs:documentation source="Name" xml:lang="EN">AgentCAStandingInstructionRequestIdentification</xs:documentation>
                        <xs:documentation source="Definition" xml:lang="EN">Identification of the linked Agent CA Standing Instruction Request for which a status is given.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="AgtCAStgInstrCxlReqId" type="DocumentIdentification8">
