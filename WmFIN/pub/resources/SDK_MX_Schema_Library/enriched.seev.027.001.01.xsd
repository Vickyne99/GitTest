            <xs:choice>
                <xs:element name="StgInstrReqSts" type="StandingInstructionStatus1Choice">
                    <xs:annotation>
                        <xs:documentation source="Name" xml:lang="EN">StandingInstructionRequestStatus</xs:documentation>
