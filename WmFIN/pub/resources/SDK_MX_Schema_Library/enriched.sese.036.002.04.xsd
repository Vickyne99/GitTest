    <xs:simpleType name="MarketClientSideCode">
        <xs:annotation>
            <xs:documentation source="Name" xml:lang="EN">MarketClientSideCode</xs:documentation>
            <xs:documentation source="Definition" xml:lang="EN">Specifies if an instruction is for a market or client side transaction.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="MAKT">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">MarketSide</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Instruction is for a market side transaction.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CLNT">
