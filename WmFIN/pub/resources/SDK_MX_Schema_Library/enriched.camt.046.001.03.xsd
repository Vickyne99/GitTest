                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">ReservationQueryDefinition</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Definition of the reservation query.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
