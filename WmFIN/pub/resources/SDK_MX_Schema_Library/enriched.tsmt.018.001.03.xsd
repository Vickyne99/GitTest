                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="SellrCtctPrsn" type="ContactIdentification1">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">SellerContactPerson</xs:documentation>
