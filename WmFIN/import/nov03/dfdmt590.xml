<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV03 MT590 -->

<field name="32C:" type="PatternType">
    <bizName>C - Value Date, Currency Code, Amount</bizName>
    <pattern>&lt;DATE2&gt;&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="32D:" type="PatternType">
    <bizName>D - Value Date, Currency Code, Amount</bizName>
    <pattern>&lt;DATE2&gt;&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="52A:" type="MixedType">
    <bizName>A - Ordering Institution</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]&lt;SWIFTBIC&gt;|&lt;NONSWIFTBIC&gt;</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="52D:" type="MixedType">
    <bizName>D - Ordering Institution</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]4*35x</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="71B:" type="PatternType">
    <bizName>B - Details of Charges</bizName>
    <pattern>6*35x</pattern>
</field>
<field name="72:" type="PatternType">
