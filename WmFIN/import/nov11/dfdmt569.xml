</field>
<field name="22F::AUTA" type="GenericFieldType">
    <bizName>Automatic Allocation Indicator</bizName>
    <pattern>/[8c]/4!c</pattern>
    <alias pattern="8c" bizName="Data Source Scheme" />    
    <alias pattern="4!c" bizName="Indicator" />            
    <choice>AUTO</choice>
    <choice>MANU</choice>
</field>
