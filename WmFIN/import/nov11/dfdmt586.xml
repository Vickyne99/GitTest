<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV03 MT586 -->
<field name="22F::PRIC" type="GenericFieldType">
    <bizName>Type of Price</bizName>
    <pattern>/[8c]/4!c</pattern>
    <alias pattern="8c" bizName="Data Source Scheme" />    
    <alias pattern="4!c" bizName="Indicator" />            
    <choice>AVER</choice>
</field>

<field name="22F::SETR" type="GenericFieldType">
    <bizName>Type of Settlement Transaction</bizName>
    <pattern>/[8c]/4!c</pattern>
    <choice>BSBK</choice>
    <choice>CNCB</choice>
    <choice>COLI</choice>
    <choice>COLO</choice>
    <choice>OWNE</choice>
    <choice>OWNI</choice>
    <choice>NETT</choice>
    <choice>PAIR</choice>
    <choice>PLAC</choice>
    <choice>PORT</choice>
    <choice>REAL</choice>
    <choice>REDM</choice>
    <choice>REPU</choice>
    <choice>RVPO</choice>
    <choice>SECB</choice>
    <choice>SECL</choice>
    <choice>SUBS</choice>
    <choice>TRAD</choice>
    <choice>TRPO</choice>
    <choice>TRVO</choice>
    <choice>TURN</choice>
    <choice>PREA</choice>
    <error type="choice">K22</error>
</field>
<field name="23G:" type="Mixed2Type">
    <bizName>Function of the Message</bizName>
    <pattern>4!c[/4!c]</pattern>
    	<alias pattern="4!c" bizName="Function" />            
    	<alias pattern="4!c" bizName="Subfunction" />                
    <choice1>CANC</choice1>
    <choice1>NEWM</choice1>
    <choice2>CODU</choice2>
    <choice2>COPY</choice2>
    <choice2>DUPL</choice2>
    <choice2>PORT</choice2>
    <error type="choice">T86</error>
</field>

<field name="94B::TRAD" type="GenericFieldType">
    <bizName>Place B - Place of Trade</bizName>
    <pattern>/[8c]/4!c[/30x]</pattern>
    <alias pattern="8c" bizName="Data Source Scheme" />            
    <alias pattern="4!c" bizName="Place Code" />                
    <alias pattern="30x" bizName="Narrative" />                    
    <choice>EXCH</choice>
    <choice>OTCO</choice>
    <choice>PRIM</choice>
    <choice>SECM</choice>
    <choice>VARI</choice>
</field>

<field name="97B::SAFE" type="PatternType">
    <bizName>Account B - Safekeeping Account</bizName>
    <pattern>/[8c]/4!c/35x</pattern>
    <alias pattern="8c" bizName="Data Source Scheme" />                
    <alias pattern="4!c" bizName="Account Type Code" />                    
    <alias pattern="35x" bizName="Account Number" />                           
</field>

<field name="98B::SETT" type="GenericFieldType">
    <bizName>Date/Time B - Settlement Date/Time</bizName>
    <pattern>/[8c]/4!c</pattern>
    <alias pattern="8c" bizName="Data Source Scheme" />        
    <alias pattern="4!c" bizName="Date Code" />        
    <choice>WISS</choice>
</field>

<field name="94B::PLIS" type="GenericFieldType">
    <bizName>Place B - Place Of Listing</bizName>
    <pattern>/[8c]/4!c[/30x]</pattern>
    <choice>EXCH</choice>
    <choice>OTCO</choice>
</field>

<field name="12B::OPST" type="GenericFieldType">
    <bizName>Type of Financial Instrument B - Option Style</bizName>
    <pattern>/[8c]/4!c</pattern>
    <alias pattern="8c" bizName="Data Source Scheme" />    
    <alias pattern="4!c" bizName="Instrument Type Code" />            
    <choice>AMER</choice>
    <choice>EURO</choice>
</field>

<field name="22F::STCO" type="GenericFieldType">
    <bizName>Settlement Transaction Condition</bizName>
    <pattern>/[8c]/4!c</pattern>
    <alias pattern="8c" bizName="Data Source Scheme" />    
    <alias pattern="4!c" bizName="Indicator" />            
    <choice>CLEN</choice>
    <choice>DIRT</choice>
    <choice>DLWM</choice>
    <choice>DRAW</choice>
    <choice>NOMC</choice>
    <choice>NPAR</choice>
    <choice>PART</choice>
    <choice>PHYS</choice>
    <choice>SPDL</choice>
    <choice>SPST</choice>
    <error type="choice">K22</error>
</field>

<field name="22F::CASY" type="GenericFieldType">
    <bizName>Cash Settlement System Indicator</bizName>
    <pattern>/[8c]/4!c</pattern>
    <alias pattern="8c" bizName="Data Source Scheme" />    
    <alias pattern="4!c" bizName="Indicator" />            
    <choice>GROS</choice>
    <choice>NETS</choice>
</field>

<field name="22F::REPT" type="MixedType">
    <bizName>Repurchase Type Indicator</bizName>
    <pattern>/[8c]/4!c</pattern>
    <alias pattern="8c" bizName="Data Source Scheme" />    
    <alias pattern="4!c" bizName="Indicator" />            
    <choice>CADJ</choice>
    <choice>TOPU</choice>
    <choice>WTHD</choice>
    <error type="choice">K22</error>
</field>


</constraints>
