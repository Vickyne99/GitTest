<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV02 MT362 -->

<field name="20:" type="PatternType">
    <bizName>Reference - Sender's Reference</bizName>
    <pattern>16x</pattern>
</field>
<field name="22A:" type="MixedType">
    <bizName>Type of Operation</bizName>
    <pattern>4!c</pattern>
    <choice>AMND</choice>
    <choice>CANC</choice>
    <choice>DUPL</choice>
    <choice>NEWT</choice>
    <error type="choice">T36</error>
</field>
<field name="30V:" type="PatternType">
    <bizName>V - Effective Date</bizName>
    <pattern>&lt;DATE4&gt;</pattern>
</field>
<field name="30X:" type="PatternType">
    <bizName>X - Period Start Date</bizName>
    <pattern>&lt;DATE4&gt;</pattern>
</field>
<field name="32H:" type="PatternType">
    <bizName>H - Currency, Interest Amount</bizName>
    <pattern>[N]&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="33E:" type="PatternType">
    <bizName>Currency, Principal Exchange Amount</bizName>
    <pattern>&lt;CUR&gt;&lt;NUMBER&gt;15</pattern>
</field>
<field name="37G:" type="PatternType">
    <bizName>G - Reset Rate</bizName>
    <pattern>[N]&lt;NUMBER&gt;12</pattern>
</field>
<field name="37M:" type="PatternType">
    <bizName>M - Total Rate</bizName>
    <pattern>[N]&lt;NUMBER&gt;12</pattern>
</field>
<field name="72:" type="PatternType">
    <bizName>Sender to Receiver Information</bizName>
    <pattern>&lt;FLD72&gt;</pattern>
</field>
<field name="94A:" type="MixedType">
    <bizName>Place A - Scope of Operation</bizName>
    <pattern>4!c</pattern>
    <choice>AGNT</choice>
    <choice>BILA</choice>
</field>
</constraints>
