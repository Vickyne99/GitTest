<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV02 MT500 -->
<field name="22F::PADI" type="GenericFieldType">
    <bizName>Payment Direction Indicator</bizName>
    <pattern>/[8c]/4!c</pattern>
</field>
<field name="23G:" type="Mixed2Type">
    <bizName>Function of the Message</bizName>
    <pattern>4!c[/4!c]</pattern>
    <choice1>CANC</choice1>
    <choice1>NEWM</choice1>
    <choice2>CODU</choice2>
    <choice2>COPY</choice2>
    <choice2>DUPL</choice2>
    <error type="choice">T86</error>
</field>
<field name="95S::ALTE" type="GenericFieldType">
    <bizName>Party S - Alternate Identification</bizName>
    <pattern>/[8c]/4!c/&lt;CC&gt;/30x</pattern>
</field>
</constraints>