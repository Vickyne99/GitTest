<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV03 MT813 -->

<field name="23:" type="PatternType">
    <bizName>Refund Purchase Agreement ID</bizName>
    <pattern>16x</pattern>
</field>
<field name="26A:" type="PatternType">
    <bizName>Refund Serial Numbers</bizName>
    <pattern>16x[/4!x]</pattern>
</field>
<field name="30:" type="PatternType">
    <bizName>Date - Refund Date</bizName>
    <pattern>&lt;DATE2&gt;</pattern>
</field>
<field name="32A:" type="PatternType">
    <bizName>A - Reimbursement Amount</bizName>
    <pattern>&lt;DATE2&gt;&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="33B:" type="PatternType">
    <bizName>B - T/C Refund Amount</bizName>
    <pattern>&lt;CUR&gt;&lt;NUMBER&gt;15</pattern>
</field>
<field name="34B:" type="PatternType">
    <bizName>B - Cash Refund Issued</bizName>
    <pattern>&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="51A:" type="MixedType">
    <bizName>A - Refund Agent</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]&lt;SWIFTBIC&gt;|&lt;NONSWIFTBIC&gt;</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="51C:" type="PatternType">
    <bizName>C - Refund Agent</bizName>
    <pattern>/34x</pattern>
</field>
<field name="57A:" type="MixedType">
    <bizName>A - Account With Institution</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]&lt;SWIFTBIC&gt;|&lt;NON-SWIFTBIC&gt;</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="57B:" type="MixedType">
    <bizName>B - Account With Institution</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF][35x]</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="57D:" type="MixedType">
    <bizName>D - Account With Institution</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]4*35x</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="59:" type="PatternType">
    <bizName>Claimant (Contact Address)</bizName>
    <pattern>[/34xCRLF]4*35x</pattern>
</field>
<field name="72:" type="PatternType">
    <bizName>Sender to Receiver Information</bizName>
    <pattern>&lt;ERI_F72narrative&gt;</pattern>
</field>
<field name="73:" type="PatternType">
    <bizName>Amounts Added</bizName>
    <pattern>6*35x</pattern>
</field>

</constraints>