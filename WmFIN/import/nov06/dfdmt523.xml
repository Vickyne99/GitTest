<?xml version="1.0"?>
<constraints>


<!-- Text Block for MT523 -->
<field name="20:" type="PatternType">
    <bizName>Reference - Transaction Reference Number</bizName>
    <pattern>16x</pattern>
</field>
<field name="21:" type="PatternType">
    <bizName>Reference - Related Reference</bizName>
    <pattern>16x</pattern>
</field>
<field name="30:" type="PatternType">
    <bizName>Date - Settlement Date</bizName>
    <pattern>&lt;DATE2&gt;</pattern>
</field>
<field name="31P:" type="PatternType">
    <bizName>Date P - Date and Place of Trade</bizName>
    <pattern>&lt;DATE2&gt;[29x]</pattern>
</field>
<field name="32B:" type="PatternType">
    <bizName>B - Settlement Amount</bizName>
    <pattern>&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="32M:" type="PatternType">
    <bizName>T - Deal Amount</bizName>
    <pattern>&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="33T:" type="PatternType">
    <bizName>Deal Price</bizName>
    <pattern>3!a&lt;NUMBER&gt;15</pattern>
    <alias pattern="3!a" bizName="CUR" />    
</field>
<field name="33V:" type="PatternType">
    <bizName>Book Value</bizName>
    <pattern>3!a&lt;AMOUNT&gt;15</pattern>
    <alias pattern="3!a" bizName="CUR" />    
</field>
<field name="34G:" type="PatternType">
    <bizName>G - Accrued Interest</bizName>
    <pattern>[3!nCRLF]&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="34H:" type="PatternType">
    <bizName>H - Accrued Interest</bizName>
    <pattern>[3!nCRLF]&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="35A:" type="PatternType">
    <bizName>A - Quantity of Securities</bizName>
    <pattern>3!a&lt;NUMBER&gt;15</pattern>
    <choice>BON</choice>
    <choice>CER</choice>
    <choice>CPN</choice>
    <choice>FMT</choice>
    <choice>MSC</choice>
    <choice>OPC</choice>
    <choice>OPS</choice>
    <choice>PRC</choice>
    <choice>PRS</choice>
    <choice>RTE</choice>
    <choice>RTS</choice>
    <choice>SHS</choice>
    <choice>UNT</choice>
    <choice>WTS</choice>
</field>
<field name="35B:" type="PatternType">
    <bizName>B - Identification of Securities</bizName>
    <pattern>&lt;35B/ISITC&gt;</pattern>
</field>
<field name="35C:" type="PatternType">
    <bizName>C - Next Coupon</bizName>
    <pattern>3!c</pattern>
</field>
<field name="35D:" type="PatternType">
    <bizName>D - Next Coupon</bizName>
    <pattern>&lt;DATE2&gt;</pattern>
</field>
<field name="35E:" type="PatternType">
    <bizName>E - Certificate Numbers</bizName>
    <pattern>6*50x</pattern>
</field>
<field name="53A:" type="PatternType">
    <bizName>A - Account for Payment</bizName>
    <pattern>[/2x][/34x][CRLF]&lt;SWIFTBIC&gt;|&lt;NONSWIFTBIC&gt;</pattern>
    <choiceList>
    	<pattern>2x</pattern>
    	<choice>C</choice>
    	<choice>D</choice>
    	<choice>RC</choice>
    	<choice>RD</choice>
    </choiceList>
</field>
