<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV03 MT558 -->
<field name="22F::AUTA" type="GenericFieldType">
    <bizName>Automatic Allocation Indicator</bizName>
    <pattern>/[8c]/4!c</pattern>
    <choice>AUTO</choice>
    <choice>MANU</choice>
</field>
<field name="22F::FSSA" type="GenericFieldType">
    <bizName>Failed Settlement Salvation Indicator</bizName>
    <pattern>/[8c]/4!c</pattern>
    <choice>FSSN</choice>
    <choice>FSSY</choice>
</field>
<field name="23G:" type="Mixed2Type">
    <bizName>Function of the Message</bizName>
    <pattern>4!c[/4!c]</pattern>
    	<alias pattern="4!c" bizName="Function" />            
    	<alias pattern="4!c" bizName="Subfunction" />                
    <choice1>ADVD</choice1>
    <choice1>CAST</choice1>
    <choice1>INST</choice1>
    <choice1>REST</choice1>
    <choice2>CODU</choice2>
    <choice2>COPY</choice2>
    <choice2>DUPL</choice2>
    <error type="choice">T86</error>
</field>
<field name="24B::CAND" type="GenericFieldType">
    <bizName>Cancellation Reason</bizName>
    <pattern>/[8c]/4!c</pattern>
    <choice>CANI</choice>
    <choice>CANS</choice>
    <choice>CSUB</choice>
    <choice>NARR</choice>
</field>
<field name="24B::DEND" type="GenericFieldType">
    <bizName>Denied Reason</bizName>
    <pattern>/[8c]/4!c</pattern>
    <choice>DCAL</choice>
    <choice>DCAN</choice>
    <choice>DFOR</choice>
	<choice>DPRG</choice>
    <choice>DREP</choice>
    <choice>DSET</choice>
	<choice>LATE</choice>
    <choice>NARR</choice>
</field>
<field name="24B::NMAT" type="GenericFieldType">
    <bizName>Unmatched Reason</bizName>
    <pattern>/[8c]/4!c</pattern>
    <choice>CPCA</choice>
</field>
<field name="24B::PEND" type="GenericFieldType">
    <bizName>Pending Reason</bizName>
    <pattern>/[8c]/4!c</pattern>
    <choice>FUTU</choice>
    <choice>PRCY</choice>
    <choice>PREA</choice>
    <choice>SFRZ</choice>
</field>
<field name="24B::REJT" type="GenericFieldType">
    <bizName>Rejection Reason</bizName>
    <pattern>/[8c]/4!c</pattern>
    <choice>CURR</choice>
    <choice>DDAT</choice>
    <choice>DTRD</choice>
	<choice>NARR</choice>
    <choice>PTYR</choice>
    <choice>REFE</choice>
	<choice>RERT</choice>
    <choice>TERM</choice>
    <choice>TRXR</choice>
	<choice>TRXT</choice>
</field>
<field name="25D::IPRC" type="GenericFieldType">
    <bizName>Instruction Processing Status</bizName>
    <pattern>/[8c]/4!c</pattern>
    <choice>CAND</choice>
    <choice>FUTU</choice>
    <choice>PACK</choice>
    <choice>CANP</choice>
    <choice>CPRC</choice>
    <choice>REJT</choice>
    <choice>TREA</choice>
</field>
<field name="25D::MOVE" type="GenericFieldType">
    <bizName>Movement Status</bizName>
    <pattern>/[8c]/4!c</pattern>
    <choice>LIMB</choice>
    <choice>CASH</choice>
    <choice>CURR</choice>
    <choice>NLEG</choice>
    <choice>EXCL</choice>
    <choice>TAXE</choice>
    <choice>AMTI</choice>
    <choice>MANU</choice>
    <choice>WAIT</choice>
    <choice>FUTU</choice>
</field>
<field name="25D::SETT" type="GenericFieldType">
    <bizName>Settlement Status</bizName>
    <pattern>/[8c]/4!c</pattern>
    <alias pattern="8c" bizName="Data Source Scheme" />    
    <alias pattern="4!c" bizName="Status Code" />        
