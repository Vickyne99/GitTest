<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV03 MT330 -->
<field name="14D:" type="MixedType">
    <bizName>Day Count Fraction</bizName>
    <pattern>7x</pattern>
    <choice>ACT/365</choice>
    <choice>AFI/365</choice>
    <choice>ACT/360</choice>
    <choice>360/360</choice>
    <choice>30E/360</choice>
</field>
<field name="20:" type="PatternType">
    <bizName>Reference - Sender's Reference</bizName>
    <pattern>16x</pattern>
    <error type="content">T26</error>
</field>
<field name="22A:" type="MixedType">
    <bizName>Type of Operation</bizName>
    <pattern>4!c</pattern>
    <choice>AMND</choice>
    <choice>CANC</choice>
    <choice>DUPL</choice>
    <choice>NEWT</choice>
    <error type="choice">T36</error>
</field>
<field name="22B:" type="MixedType">
    <bizName>Type of Event</bizName>
    <pattern>4!c</pattern>
    <choice>CHNG</choice>
    <choice>CINT</choice>
    <choice>CONF</choice>
    <choice>SETT</choice>
</field>
<field name="23G:" type="Mixed2Type">
    <bizName>Function of the Message</bizName>
    <pattern>4!c[/4!c]</pattern>
    <alias pattern="4!c" bizName="Function" />            
    <alias pattern="4!c" bizName="Subfunction" />                
    <choice1>CANC</choice1>
    <choice1>NEWM</choice1>
    <choice2>CODU</choice2>
    <choice2>COPY</choice2>
    <choice2>DUPL</choice2>
    <error type="choice">T86</error>
</field>
<field name="24D:" type="MixedType">
    <bizName>Dealing Method</bizName>
    <pattern>4!c[/35x]</pattern>
    <choice>ELEC</choice>
    <choice>PHON</choice>
    <error type="choice">T68</error>
</field>
<field name="30F:" type="PatternType">
    <bizName>F - Last Day of the Next Interest Period</bizName>
    <pattern>&lt;DATE4&gt;</pattern>
</field>
<field name="30X:" type="PatternType">
    <bizName>X - Interest Due Date</bizName>
    <pattern>&lt;DATE4&gt;</pattern>
</field>
<field name="32B:" type="PatternType">
    <bizName>B - Currency and Balance</bizName>
    <pattern>&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="33B:" type="PatternType">
    <bizName>B - Transaction Currency and Net Interest Amount</bizName>
    <pattern>&lt;CUR&gt;&lt;NUMBER&gt;15</pattern>
</field>
<field name="37L:" type="PatternType">
    <bizName>L - Tax Rate</bizName>
    <pattern>&lt;NUMBER&gt;12</pattern>
</field>
<field name="72:" type="PatternType">
    <bizName>Sender to Receiver Information</bizName>
    <pattern>&lt;FLD72&gt;</pattern>
</field>
<field name="86A:" type="MixedType">
    <bizName>A - Intermediary 2</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]&lt;SWIFTBIC&gt;|&lt;NONSWIFTBIC&gt;</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="86D:" type="MixedType">
    <bizName>D - Intermediary 2</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]4*35x</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="94A:" type="MixedType">
    <bizName>Place A - Scope of Operation</bizName>
    <pattern>4!c</pattern>
    <choice>AGNT</choice>
    <choice>BILA</choice>
</field>
</constraints>
