    <pattern>[/1!a][/34x][CRLF][35x]</pattern>
</field>
<field name="57C:" type="PatternType">
    <bizName>C - Account With Institution</bizName>
    <pattern>/34x</pattern>
</field>
<field name="57D:" type="MixedType">
    <bizName>D - Account With Institution</bizName>
    <pattern>[/1!a][/34x][CRLF]4*35x</pattern>
</field>

<field name="59A:" type="PatternType">
    <bizName>A - Beneficiary Customer</bizName>
    <pattern>[/34x][CRLF](&lt;SWIFTBIC&gt;|&lt;NONSWIFTBIC&gt;)</pattern>
    <alias pattern="34x" bizName="Account" />    
</field>
</constraints>
