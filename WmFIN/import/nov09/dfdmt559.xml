<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV03 MT559 -->

<field name="23:" type="ChoiceType">
    <bizName>Further Identification</bizName>
    <pattern>16x</pattern>
    <choice>INCOME</choice>
    <choice>INCPRINC</choice>
    <choice>PRINCIPL</choice>
</field>
<field name="31C:" type="PatternType">
    <bizName>C - Payable Date</bizName>
    <pattern>&lt;DATE2&gt;</pattern>
</field>
<field name="31E:" type="PatternType">
    <bizName>E - Record Date</bizName>
    <pattern>&lt;DATE2&gt;</pattern>
</field>
<field name="31L:" type="PatternType">
    <bizName>L - Coupon Clipping Date</bizName>
    <pattern>&lt;DATE2&gt;</pattern>
</field>
<field name="32G:" type="PatternType">
    <bizName>Gross Amount</bizName>
    <pattern>&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="33T:" type="PatternType">
    <bizName>Redemption Price</bizName>
    <pattern>&lt;CUR&gt;&lt;NUMBER&gt;15</pattern>
</field>
<field name="34A:" type="PatternType">
    <bizName>Net Proceeds</bizName>
    <pattern>&lt;DATE2&gt;&lt;CUR&gt;&lt;NUMBER&gt;15</pattern>
</field>
<field name="53A:" type="MixedType">
    <bizName>A - Account for Payment</bizName>
    <pattern>[&lt;PartyIdentifier&gt;][CRLF]&lt;SWIFTBIC&gt;</pattern>
</field>
<field name="53C:" type="PatternType">
    <bizName>C - Account for Payment</bizName>
    <pattern>/34x</pattern>
</field>
<field name="53D:" type="MixedType">
    <bizName>D - Account for Payment</bizName>
    <pattern>[&lt;PartyIdentifier&gt;][CRLF]4*35x</pattern>
</field>
<field name="57A:" type="MixedType">
    <bizName>A - Account With Institution</bizName>
    <pattern>[&lt;PartyIdentifier&gt;][CRLF]&lt;SWIFTBIC&gt;</pattern>
</field>
<field name="57D:" type="MixedType">
    <bizName>D - Account With Institution</bizName>
    <pattern>[&lt;PartyIdentifier&gt;][CRLF]4*35x</pattern>
</field>
<field name="71B:" type="PatternType">
    <bizName>Own Charges</bizName>
    <pattern>6*35x</pattern>
</field>
<field name="71C:" type="PatternType">
    <bizName>Other Charges</bizName>
    <pattern>6*35x</pattern>
</field>
<field name="72:" type="PatternType">
    <bizName>Sender to Receiver Information</bizName>
    <pattern>&lt;ERI_F72narrative&gt;</pattern>
</field>
</constraints>
