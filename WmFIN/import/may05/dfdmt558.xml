<field name="22F::MICO" type="GenericFieldType">
    <bizName>Indicator F - Method of Interest Computation</bizName>
    <pattern>/[8c]/4!c</pattern>
    <choice>A001</choice>
    <choice>A002</choice>
    <choice>A003</choice>
    <choice>A004</choice>
    <choice>A005</choice>
    <choice>A006</choice>
    <choice>A007</choice>
</field>
<field name="22H::COLA" type="MixedType">
