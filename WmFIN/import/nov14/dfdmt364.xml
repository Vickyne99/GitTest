<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV03 MT364 -->

<field name="20:" type="PatternType">
    <bizName>Reference - Sender's Reference</bizName>
    <pattern>16x</pattern>
</field>
<field name="22A:" type="MixedType">
    <bizName>Type of Operation</bizName>
    <pattern>4!c</pattern>
    <choice>AMND</choice>
    <choice>CANC</choice>
    <choice>DUPL</choice>
    <choice>NEWT</choice>
    <error type="choice">T36</error>
</field>
<field name="22B:" type="PatternType">
    <bizName>Type of Event</bizName>
    <pattern>4!c</pattern>
</field>
<field name="22D:" type="PatternType">
    <bizName>Accrual of Interest Specification</bizName>
    <pattern>4!c</pattern>
    <choice>LAST</choice>
    <choice>NEXT</choice>
    <choice>OTHR</choice>
</field>
<field name="23A:" type="PatternType">
    <bizName>Identification of the Swap</bizName>
    <pattern>('CAPBUYER'|'CAPSELLER'|'COLLARBYER'|'COLLARSLLR'|'FIXEDFIXED'|'FIXEDFLOAT'|'FLOATFIXED'|'FLOATFLOAT'|'FLOORBUYER'|'FLOORSLLER')/'NET'</pattern>
</field>
<field name="30P:" type="PatternType">
    <bizName>Original Termination Date</bizName>
    <pattern>&lt;DATE4&gt;</pattern>
</field>
<field name="30Q:" type="PatternType">
    <bizName>Termination/Recouponing Effective Date</bizName>
    <pattern>&lt;DATE4&gt;</pattern>
</field>
<field name="30T:" type="PatternType">
    <bizName>Termination/Recouponing Trade Date</bizName>
    <pattern>&lt;DATE4&gt;</pattern>
</field>
<field name="30V:" type="PatternType">
    <bizName>Original Effective Date</bizName>
    <pattern>&lt;DATE4&gt;</pattern>
</field>
<field name="32B:" type="PatternType">
    <bizName>Current Currency, Notional Amount</bizName>
    <pattern>&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="32G:" type="PatternType">
    <bizName>New Currency, Notional Amount</bizName>
    <pattern>&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="37U:" type="PatternType">
    <bizName>Current Fixed Rate</bizName>
    <pattern>&lt;NUMBER&gt;12</pattern>
</field>
<field name="72:" type="PatternType">
    <bizName>Sender to Receiver Information</bizName>
    <pattern>&lt;FLD72&gt;</pattern>
</field>
<field name="94A:" type="MixedType">
    <bizName>Place A - Scope of Operation</bizName>
    <pattern>4!c</pattern>
    <choice>AGNT</choice>
    <choice>BILA</choice>
</field>
</constraints>
