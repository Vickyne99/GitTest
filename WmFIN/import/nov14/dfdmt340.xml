<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV03 MT340 -->
<field name="17F:" type="MixedType">
    <bizName>FRA Discounting</bizName>
    <pattern>1!a</pattern>
    <choice>N</choice>
    <choice>Y</choice>
    <error type="choice">T36</error>
</field>
<field name="20:" type="PatternType">
    <bizName>Reference - Sender's Reference</bizName>
    <pattern>16x</pattern>
    <error type="content">T26</error>
</field>
<field name="22A:" type="MixedType">
    <bizName>Type of Operation</bizName>
    <pattern>4!c</pattern>
    <choice>AMND</choice>
    <choice>CANC</choice>
    <choice>NEWT</choice>
    <error type="choice">T36</error>
</field>
<field name="30F:" type="PatternType">
    <bizName>F - Effective Date</bizName>
    <pattern>&lt;DATE4&gt;</pattern>
</field>
<field name="30V:" type="PatternType">
    <bizName>V - Fixing Date</bizName>
    <pattern>&lt;DATE4&gt;</pattern>
</field>
<field name="32B:" type="PatternType">
    <bizName>B - Currency, Notional Amount</bizName>
    <pattern>&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="38G:" type="PatternType">
    <bizName>Designated Maturity</bizName>
    <pattern>&lt;PERIOD&gt;/&lt;PERIOD&gt;</pattern>
</field>
<field name="71F:" type="PatternType">
    <bizName>F - Broker's Commission</bizName>
    <pattern>&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="72:" type="PatternType">
    <bizName>Sender to Receiver Information</bizName>
    <pattern>&lt;FLD72&gt;</pattern>
</field>
<field name="86A:" type="MixedType">
    <bizName>A - Intermediary 2</bizName>
    <pattern>[&lt;PartyIdentifier&gt;][CRLF]&lt;SWIFTBIC&gt;</pattern>
</field>
<field name="86D:" type="MixedType">
    <bizName>D - Intermediary 2</bizName>
    <pattern>[&lt;PartyIdentifier&gt;][CRLF]4*35x</pattern>
</field>

<field name="77H:" type="MixedType">
    <bizName>Type, Date, Version of the Agreement</bizName>
    <pattern>6a[/&lt;DATE4&gt;][//4!n]</pattern>
    <choiceList>
    	<pattern>6a</pattern>
	<choice>AFB</choice>
	<choice>DERV</choice>
	<choice>EMA</choice>
	<choice>FRABBA</choice>
	<choice>ISDA</choice>
	<choice>ISDACN</choice>
	<choice>OTHR</choice>
    </choiceList>
</field>
<field name="91A:" type="MixedType">
    <bizName>A - Reporting Party</bizName>
    <pattern>[&lt;PartyIdentifier&gt;][CRLF]&lt;SWIFTBIC&gt;</pattern>
</field>
<field name="91D:" type="MixedType">
    <bizName>D - Reporting Party</bizName>
    <pattern>[&lt;PartyIdentifier&gt;][CRLF]4*35x</pattern>
</field>
<field name="91J:" type="MixedType">
    <bizName>J - Reporting Party</bizName>
    <pattern>&lt;PARTYFLD/J&gt;</pattern>
    <choice>/ABIC/</choice>
    <choice>/CLRC/</choice>
    <choice>/LEIC/</choice>
    <choice>/NAME/</choice>
    <error type="choice">T78</error>
</field>
</constraints>
