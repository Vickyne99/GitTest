<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV03 MT542  -->

<field name="98A::CERT" type="PatternType">
    <bizName>Date/Time A - Certification Date/Time </bizName>
    <pattern>//&lt;DATE4&gt;</pattern>
</field>
<field name="98B::CERT" type="GenericFieldType">
    <bizName>Date/Time B - Certification Date/Time </bizName>
    <pattern>/[8c]/4!c</pattern>  
</field>
<field name="98C::CERT" type="PatternType">
    <bizName>Date/Time C - Certification Date/Time </bizName>
    <pattern>//&lt;DATE4&gt;&lt;TIME2&gt;</pattern>
</field>

<field name="22F::TRCA" type="GenericFieldType">
    <bizName>Party Capacity Indicator</bizName>
    <pattern>/[8c]/4!c</pattern>
    <choice>INFI</choice>
    <choice>MKTM</choice>
    <choice>MLTF</choice>
    <choice>RMKT</choice>
    <choice>SINT</choice>
    <choice>TAGT</choice>
</field>

<field name="22F::COLA" type="GenericFieldType">
    <bizName>Exposure Type Indicator</bizName>
    <pattern>/[8c]/4!c</pattern>
    <alias pattern="8c" bizName="Data Source Scheme" />    
    <alias pattern="4!c" bizName="Indicator" />            
    <choice>COMM</choice>
    <choice>CRPR</choice>
    <choice>FUTR</choice>
    <choice>OPTN</choice>
    <choice>REPO</choice>
    <choice>SECL</choice>
    <choice>SECB</choice>
    <choice>SWAP</choice>
</field>

</constraints>