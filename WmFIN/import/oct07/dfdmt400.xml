<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV03 MT 400 -->
<field name="20:" type="PatternType">
    <bizName>Sending Bank's TRN</bizName>
    <pattern>16x</pattern>
</field>
<field name="32a:" type="Type">
    <bizName>Amount Collected</bizName>
    <pattern>A, B or K</pattern></field>
<field name="32A:" type="PatternType">
    <bizName>A - Amount Collected</bizName>
    <pattern>&lt;DATE2&gt;&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="32B:" type="PatternType">
    <bizName>B - Amount Collected</bizName>
    <pattern>&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="32K:" type="Mixed2Type">
    <bizName>K - Amount Collected</bizName>
    <pattern>1!a3!n2!a&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
    <choice1>D</choice1>
    <choice1>M</choice1>
    <choice2>BE</choice2>
    <choice2>CC</choice2>
    <choice2>FD</choice2>
    <choice2>FP</choice2>
    <choice2>GA</choice2>
    <choice2>ID</choice2>
    <choice2>ST</choice2>
    <choice2>TD</choice2>
    <choice2>XX</choice2>
</field>
<field name="52A:" type="MixedType">
    <bizName>A - Ordering Bank</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]&lt;SWIFTBIC&gt;|&lt;NONSWIFTBIC&gt;</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="52D:" type="MixedType">
    <bizName>D - Ordering Bank</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]4*35x</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="53A:" type="MixedType">
    <bizName>A - Sender's Correspondent</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]&lt;SWIFTBIC&gt;|&lt;NON-SWIFTBIC&gt;</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="53D:" type="MixedType">
    <bizName>D - Sender's Correspondent</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]4*35x</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="57A:" type="MixedType">
    <bizName>A - Account With Bank</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]&lt;SWIFTBIC&gt;|&lt;NON-SWIFTBIC&gt;</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="57D:" type="MixedType">
    <bizName>D - Account With Bank</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]4*35x</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="58A:" type="MixedType">
    <bizName>A - Beneficiary Bank</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]&lt;SWIFTBIC&gt;|&lt;NONSWIFTBIC&gt;</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="58B:" type="MixedType">
    <bizName>B - Beneficiary Bank</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF][35x]</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="58D:" type="MixedType">
    <bizName>D - Beneficiary Bank</bizName>
    <pattern>[&lt;BICPLUS&gt;][CRLF]4*35x</pattern>
    <choice>C</choice>
    <choice>D</choice>
</field>
<field name="71B:" type="PatternType">
    <bizName>B - Details of Charges</bizName>
    <pattern>6*35x</pattern>
</field>
<field name="72:" type="PatternType">
    <bizName>Sender to Receiver Information</bizName>
    <pattern>&lt;ERI_F72narrative&gt;</pattern>
</field>
<field name="73:" type="PatternType">
    <bizName>Details of Amounts Added</bizName>
    <pattern>6*35x</pattern>
</field>
</constraints>
