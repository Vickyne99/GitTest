<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV03 MT516 -->

<field name="26H:" type="PatternType">
    <bizName>Collateral Type</bizName>
    <pattern>16x</pattern>
</field>
<field name="30:" type="PatternType">
    <bizName>Date - Effective Date</bizName>
    <pattern>&lt;DATE2&gt;</pattern>
</field>
<field name="31F:" type="PatternType">
    <bizName>Expiry and Option Date of Loan</bizName>
    <pattern>&lt;DATE2&gt;[/&lt;DATE2&gt;][//35x]</pattern>
</field>
<field name="32A:" type="PatternType">
    <bizName>A - Cash Collateral Amount</bizName>
    <pattern>&lt;DATE2&gt;&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="32B:" type="PatternType">
    <bizName>B - Cash Collateral Amount</bizName>
    <pattern>&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="35A:" type="MixedType">
    <bizName>Quantity of Securities on Loan</bizName>
    <pattern>3!a&lt;NUMBER&gt;15</pattern>
    <choice>BON</choice>
    <choice>CER</choice>
    <choice>CPN</choice>
    <choice>FMT</choice>
    <choice>MSC</choice>
    <choice>OPC</choice>
    <choice>OPS</choice>
    <choice>PRC</choice>
    <choice>PRS</choice>
    <choice>RTE</choice>
    <choice>RTS</choice>
    <choice>SHS</choice>
    <choice>UNT</choice>
    <choice>WTS</choice>
</field>
<field name="35S:" type="MixedType">
    <bizName>Quantity of Securities Transferred as Collateral</bizName>
    <pattern>3!a&lt;NUMBER&gt;15</pattern>
    <choice>BON</choice>
    <choice>CER</choice>
    <choice>CPN</choice>
    <choice>FMT</choice>
    <choice>MSC</choice>
    <choice>OPC</choice>
    <choice>OPS</choice>
    <choice>PRC</choice>
    <choice>PRS</choice>
    <choice>RTE</choice>
    <choice>RTS</choice>
    <choice>SHS</choice>
    <choice>UNT</choice>
    <choice>WTS</choice>
</field>
<field name="37J:" type="PatternType">
    <bizName>Interest Rate on Securities</bizName>
    <pattern>&lt;NUMBER&gt;12</pattern>
</field>
<field name="57A:" type="MixedType">
    <bizName>A - Account With Institution</bizName>
    <pattern>[&lt;PartyIdentifier&gt;][CRLF]&lt;SWIFTBIC&gt;</pattern>
</field>
<field name="57D:" type="MixedType">
    <bizName>D - Account With Institution</bizName>
    <pattern>[&lt;PartyIdentifier&gt;][CRLF]4*35x</pattern>
</field>
<field name="72:" type="PatternType">
    <bizName>Sender to Receiver Information</bizName>
    <pattern>&lt;ERI_F72narrative&gt;</pattern>
</field>
<field name="77D:" type="PatternType">
    <bizName>Special Terms and Conditions</bizName>
    <pattern>6*35x</pattern>
</field>
<field name="87A:" type="MixedType">
    <bizName>A - Receiver/Deliverer of Securities on Loan</bizName>
    <pattern>[&lt;PartyIdentifier&gt;][CRLF]&lt;SWIFTBIC&gt;</pattern>
</field>
<field name="87D:" type="MixedType">
    <bizName>D - Receiver/Deliverer of Securities on Loan</bizName>
    <pattern>[&lt;PartyIdentifier&gt;][CRLF]4*35x</pattern>
</field>

</constraints>