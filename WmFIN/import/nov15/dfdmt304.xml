</field>
<field name="98G:" type="Mixed2Type">
    <bizName>Confirmation Timestamp</bizName>
    <pattern>&lt;DATE4&gt;&lt;TIME2&gt;[1!x][3n][/[1!x]2!n[2!n]]</pattern>
    <alias pattern="&lt;DATE4&gt;" bizName="Execution Date" />                
    <alias pattern="&lt;TIME2&gt;" bizName="Execution Time" />
	<alias pattern="1!x" bizName="Comma" />
	<alias pattern="1!x" bizName="Sign" />
	<choice1>,</choice1>
	<choice2>-</choice2>
     <choice2>+</choice2>
</field>
<field name="98H:" type="Mixed2Type">
    <bizName>Clearing Timestamp</bizName>
    <pattern>&lt;TIME2&gt;[1!x][3n][/[1!x]2!n[2!n]]</pattern>               
    <alias pattern="&lt;TIME2&gt;" bizName="Execution Time" />
	<alias pattern="1!x" bizName="Comma" />
	<alias pattern="1!x" bizName="Sign" />
	<choice1>,</choice1>
	<choice2>-</choice2>
     <choice2>+</choice2>
</field>
</constraints>
