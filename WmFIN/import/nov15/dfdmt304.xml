<?xml version="1.0"?>
<constraints>

<!-- Text Block for NOV03 MT304 -->

<field name="22A:" type="MixedType">
    <bizName>Type of Operation</bizName>
    <pattern>4!c</pattern>
    <choice>AMND</choice>
    <choice>CANC</choice>
    <choice>DUPL</choice>
    <choice>NEWT</choice>
    <error type="choice">T36</error>
</field>

<field name="17F:" type="MixedType">
    <bizName>Final Close Indicator</bizName>
    <pattern>1!a</pattern>
    <choice>N</choice>
    <choice>Y</choice>
    <error type="choice">T36</error>
</field>
<field name="20:" type="PatternType">
    <bizName>Reference - Sender's Reference</bizName>
    <pattern>16x</pattern>
</field>
<field name="21A:" type="PatternType">
    <bizName>Reference A - Reference to the Associated Trade</bizName>
    <pattern>16x</pattern>
    <error type="content">T26</error>
</field>
<field name="21G:" type="PatternType">
    <bizName>Reference G - Executing Broker's Reference</bizName>
    <pattern>16x</pattern>
</field>
<field name="32B:" type="PatternType">
    <bizName>B - Currency, Amount Bought</bizName>
    <pattern>&lt;CUR&gt;&lt;AMOUNT&gt;15</pattern>
</field>
<field name="33B:" type="PatternType">
    <bizName>B - Currency, Amount Sold</bizName>
